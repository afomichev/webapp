@model WebApplication1.Models.bs_SectorBank

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>bs_SectorBank</h4>
        <hr />
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.LabelFor(model => model.SectorBankName, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.SectorBankName)
                @Html.ValidationMessageFor(model => model.SectorBankName)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.GICSSubindustryID, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.GICSSubindustryID)
                @Html.ValidationMessageFor(model => model.GICSSubindustryID)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.SectorOKVEDID, "SectorOKVEDID", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("SectorOKVEDID", String.Empty)
                @Html.ValidationMessageFor(model => model.SectorOKVEDID)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
